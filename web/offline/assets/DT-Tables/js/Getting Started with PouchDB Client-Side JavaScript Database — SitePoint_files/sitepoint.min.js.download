    var pending = [];

    function id(prefix) {
        return (prefix || '') + Math.random().toString(16).substr(2);
    }

    var insert = {
        'iframe': function(url) {
            var ifr = document.createElement('iframe');
            ifr.id = id('i-');
            ifr.frameborder = '0';
            ifr.allowtransparency = 'true';
            ifr.marginheight = '0';
            ifr.marginwidth = '0';
            ifr.width = '0';
            ifr.hspace = '0';
            ifr.vspace = '0';
